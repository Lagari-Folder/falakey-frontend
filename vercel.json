{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],

  "version": 2,
  "builds": [
    { "src": "vite.config.*", "use": "@vercel/static-build" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "x-prerender": "1"
      }
    }
  ]
}
